<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_mascotasFavoritas"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="co.com.pets.fragment.MascotasFavoritas">

    <include
        android:id="@+id/actionBarMascotasFavoritas"
        layout="@layout/actionbarback"
        ></include>


    <android.support.v7.widget.RecyclerView
        android:id="@+id/rvMascotasFavoritas"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/actionBarMascotasFavoritas"
        android:layout_centerHorizontal="true"
        android:padding="@dimen/activity_horizontal_margin"
        />


</RelativeLayout>
